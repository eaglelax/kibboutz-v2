{
  "expo": {
    "name": "Kibboutz",
    "slug": "kibboutz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "kibboutz",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#22c55e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kibboutz.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#22c55e"
      },
      "package": "com.kibboutz.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-asset",
      ["expo-build-properties", {
        "android": {
          "kotlinVersion": "1.9.25"
        }
      }]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6719de45-ba5f-4799-b1a8-e5c47d33a21f"
      }
    },
    "owner": "axelnig"
  }
}
